{
    "name": "Dira Behanaha",
    "description": "Adds a column to table at https://www.dira.moch.gov.il/ProjectsList that shows precenatge to win project",
    "version": "1.0",
    "manifest_version": 3,
    "author": "Eyal Avni",
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "js/service_worker.js",
        "type": "module"
    },
    "host_permissions": ["https://www.dira.moch.gov.il/ProjectsList"],
    "action": {
        "default_icon": "img/icon.png"
    },
    "icons": {
        "48": "img/icon.png",
        "128": "img/icon.png"
    }
}
